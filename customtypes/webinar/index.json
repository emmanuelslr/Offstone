{
  "format": "custom",
  "id": "webinar",
  "label": "Webinar",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": { "type": "UID", "config": { "label": "UID" } },
      "title": { "type": "Text", "config": { "label": "Titre" } },
      "embed_video": { "type": "Embed", "config": { "label": "Vid√©o" } },
      "key_takeaways": { "type": "Group", "config": { "label": "Key takeaways", "fields": { "item": { "type": "Text", "config": { "label": "Point" } } } } },
      "slides_download": { "type": "Link", "config": { "label": "Slides", "select": "media" } },
      "event_date": { "type": "Date", "config": { "label": "Date" } },
      "seo": {
        "type": "Group",
        "config": {
          "label": "SEO",
          "fields": {
            "meta_title": { "type": "Text", "config": { "label": "Meta title" } },
            "meta_description": { "type": "Text", "config": { "label": "Meta description" } },
            "og_image": { "type": "Image", "config": { "label": "OG image", "constraint": {}, "thumbnails": [] } },
            "canonical_url": { "type": "Link", "config": { "label": "Canonical URL", "select": null } },
            "noindex": { "type": "Boolean", "config": { "label": "Noindex", "default_value": false } }
          }
        }
      }
    }
  }
}

